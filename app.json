{
  "expo": {
    "name": "AIImageForge",
    "slug": "aiimageforge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "aiimageforge",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aiimageforge.app",
      "buildNumber": "1.0.0",
      "appleTeamId": "52T7P32J34",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app requires access to your photo library to save generated images.",
        "NSCameraUsageDescription": "This app requires access to your camera to capture images.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "aiimageforge"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0A0A0F",
        "foregroundImage": "./assets/images/app-icon.png"
      },
      "package": "com.aiimageforge.app",
      "versionCode": 1
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/app-icon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-screen.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0A0A0F",
          "dark": {
            "backgroundColor": "#0A0A0F"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a3490a3d-d05f-41bb-a490-c5e69da17618"
      }
    }
  }
}
