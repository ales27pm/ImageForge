{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "platform": "ios",
      "distribution": "store",
      "autoIncrement": true,
        "credentialsSource": "remote",
        "ios": {
